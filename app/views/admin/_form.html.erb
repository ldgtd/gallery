<div class="span9 content_admin">
  <div class="content">
  <h1>New gallery</h1>
  <hr /><br/>
  <%= form_for @image_store, :url => (@image_store.new_record? ? admins_path : admin_path(@image_store.id)), :html => {:multipart => true} do |f| %>
    <div>
    	<%= f.label :name %><%= f.text_field :name %>
    </div>
    <div>
    	<%= f.label :description %><%= f.text_field :description %>
    </div>
    <div>
      <%= f.label :image  %><%= f.file_field :image %>  
    </div>
     <div>
      <%= f.label :imageZIP %><%= f.file_field :image %>  
    </div>
    <div class="actions">
      <%= f.submit "Done", :class => 'btn btn-inverse' %>
    </div>
    <div class="back">
      <%= link_to 'back', :controller => "admin", :action => "index" %>
    </div>
  <% end %>
  <div>
</div>

